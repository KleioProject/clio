{
  "name": "clio",
  "version": "1.0.0",
  "description": "Client",
  "main": "index.html",
  "scripts": {
    "develop": "webpack-dev-server --inline --progress --port 8080",
    "production": "rimraf static && webpack --config configs/webpack.prod.js --progress --profile --bail",
    "production:start": "npm run production && npm run serverprod",
    "copyServer": "copyfiles -u 2 src/ssr/favicon.ico src/ssr/server.js static",
    "ssr": "rimraf static && webpack --config configs-ssr/webpack.entry.ssr.js --progress --profile --bail && npm run copyServer",
    "ssr:watch": "rimraf static && npm run copyServer && webpack --config configs-ssr/webpack.entry.ssr.js --progress --bail --hide-modules --watch",
    "ssr:start": "npm run ssr && node static/server.js",
    "serverprod": "node backend/prod.server.js",
    "serverdev": "nodemon backend/prod.server.js",
    "serverssr": "node static/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KleioProject/clio"
  },
  "keywords": [
    "Apollo",
    "graphQL",
    "Vue",
    "Vuex",
    "Webpack"
  ],
  "author": "KleioProject",
  "license": "MIT",
  "homepage": "https://github.com/KleioProject/clio#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^3.3.7",
    "compression-webpack-plugin": "^1.0.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^1.2.0",
    "cors": "^2.8.4",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "gql-loader": "^1.0.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "normalize.css": "^7.0.0",
    "null-loader": "^0.1.1",
    "on-build-webpack": "^0.1.0",
    "optimize-css-assets-webpack-plugin": "^3.0.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-url": "^7.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "vue-loader": "^13.0.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "apollo-client": "^1.9.2",
    "axios": "^0.16.2",
    "body-parser": "^1.18.1",
    "ckeditor": "^4.7.2",
    "cookie-parser": "^1.4.3",
    "graceful-fs": "^4.1.11",
    "graphql-tag": "^2.4.2",
